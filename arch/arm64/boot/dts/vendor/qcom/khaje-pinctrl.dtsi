	/* Spes Goodix Fingerprint pinctrl --end */
	
	ant-check-pin {
		ant_check_default:
		ant_check_default {
			mux {
				pins = "gpio107";
				function = "gpio";
			};
			
			config {
				pins = "gpio107";
				drive-strength = <2>;
				bias-pull-up;
				input-enable;
				input-debounce = <5000>;
			};
		};
	};
	
	ant-check-div-pin {
		ant_check_div_default:
		ant_check_div_default {
			mux {
				pins = "gpio25";
				function = "gpio";
			};
			
			config {
				input-enable;
				pins = "gpio25";
				bias-pull-up;
				input-debounce = <5000>;
			};
		};
	};
	
	fm_inside_lna_pins: fm_inside_lna_pins {
		fm_inside_lna_active: fm_inside_lna_active {
			mux {
				pins = "gpio55";
				function = "gpio";
			};
			config {
				pins = "gpio55";
				drive-strength = <2>;
				bias-pull-down;
				output-high;
