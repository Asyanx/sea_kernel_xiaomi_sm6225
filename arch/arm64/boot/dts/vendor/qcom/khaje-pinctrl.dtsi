				output-low;
			};
		};
	};
	
	nfc {
		/* Spes NFC pinctrl --start */
		nfc_int_active {
			/* active state */
			mux {
				/* GPIO 70: NFC CLOCK REQUEST */
				pins = "gpio70";
				function = "gpio";
			};
			
			config {
				pins = "gpio70";
				drive-strength = <2>; /* 2 MA */
				bias-pull-up;
			};
		};
		
		nfc_int_suspend {
			/* sleep state */
			mux {
				/* GPIO 70: NFC CLOCK REQUEST */
				pins = "gpio70";
				function = "gpio";
			};
			
			config {
				pins = "gpio70";
				drive-strength = <2>; /* 2 MA */
				bias-pull-up;
			};
		};
		
		nfc_enable_active {
			/* active state */
			mux {
				/* GPIO 69: NFC CLOCK REQUEST */
				pins = "gpio69";
				function = "gpio";
			};
			
			config {
				pins = "gpio69";
				drive-strength = <2>; /* 2 MA */
				bias-pull-up;
			};
		};
		
		nfc_enable_suspend {
			/* sleep state */
			mux {
				/* GPIO 69: NFC CLOCK REQUEST */
				pins = "gpio69";
				function = "gpio";
			};
			
			config {
				pins = "gpio69";
				drive-strength = <2>; /* 2 MA */
				bias-disable;
			};
		};
		/* Spes NFC pinctrl --end */
		nfc_clk_req_active {
			/* active state */
			mux {
				/* GPIO 71: NFC CLOCK REQUEST */
				pins = "gpio71";
				function = "gpio";
			};
			
			config {
				pins = "gpio71";
				drive-strength = <2>; /* 2 MA */
				bias-pull-up;
			};
		};
		
		nfc_clk_req_suspend {
			/* sleep state */
			mux {
				/* GPIO 71: NFC CLOCK REQUEST */
				pins = "gpio71";
				function = "gpio";
			};
			
			config {
				pins = "gpio71";
				drive-strength = <2>; /* 2 MA */
				bias-disable;
			};
		};
	};
	
	pmx_ts_reset_active {
		ts_reset_active: ts_reset_active {
			mux {
